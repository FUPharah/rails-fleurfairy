<h1> Edit your plant </h1>

<%= simple_form_for (@plant) do |f| %>

    <div class="form-inputs">
      <%= f.input :title %>
      <%= f.input :description %>
      <%= f.input :price %>
      <%= f.input :city %>
      <%= f.input :photo, as: :file %>
    </div>

    <div class="form-actions">
      <%= f.button :submit %>
    </div>
<% end %>
